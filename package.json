{
  "name": "education-eval",
  "version": "1.0.0",
  "description": "Reference implementation for AI evaluation in education - curriculum adherence and defensive evals",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:curriculum": "jest test/curriculum.test.js",
    "test:defensive": "jest test/defensive.test.js",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "eval:quick": "vibecheck evals/curriculum/5th-grade-math.yml",
    "eval:all": "vibecheck evals/"
