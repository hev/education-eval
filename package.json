  "keywords": [
    "education",
    "ai",
    "evaluation",
    "vibecheck",
    "llm",
    "curriculum",
    "safety"
  ],
  "author": "Education AI Team",
  "license": "MIT",
  "dependencies": {
    "vibecheck-runner": "^0.4.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "jest-junit": "^16.0.0",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
